

{
    "manifest_version":3,
    "name": "HelpMeOut",
    "icons":{
        "128": "icon_128x128.png"
    },
    "version": "1.0.0",
    "background": {
        "service_worker":"service-worker.js",
        "type":"module"
    },
    "action":{
        "default_popup": "popup/index.html"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}